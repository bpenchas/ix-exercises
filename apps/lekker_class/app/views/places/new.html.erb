<h1>New Place!</h1>

<%= form_for @place do |f| %>
	 <p>
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</p>

	<p>
		<%= f.label :description %><br>
		<%= f.text_area :description %>
	</p>
	<p>
		<%= f.label :address %><br>
		<%= f.text_area :address %>
	</p>
	<p>
		<%= f.label :image %><br>
		<%= f.text_area :image %>
	</p>
	 <p>
		<%= f.submit %>
	</p>

<% end %>